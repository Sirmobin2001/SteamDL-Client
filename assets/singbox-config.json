{
    "dns": {
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        },
        "independent_cache": true,
        "rules": [
            {
                "domain": [
                    "dl.gamegk.ir"
                ],
                "server": "dns-direct"
            },
            {
                "process_name": [
                    "sing-box.exe",
                    "sing-box"
                ],
                "server": "dns-direct"
            }
        ],
        "servers": [
            {
                "address": "1.1.1.1",
                "tag": "dns-direct",
                "detour": "direct"
            },
            {
                "address": "185.179.90.90",
                "detour": "direct",
                "tag": "dns-remote"
            }
        ],
        "strategy": "ipv4_only",
        "final": "dns-remote"
    },
    "inbounds": [
        {
            "auto_route": true,
            "inet4_address": "172.19.0.1/28",
            "mtu": 9000,
            "sniff": true,
            "stack": "mixed",
            "strict_route": false,
            "tag": "tun-in",
            "type": "tun",
            "interface_name": "singbox_tun"
        }
    ],
    "log": {
        "level": "trace",
        "output": "singbox.log",
        "timestamp": true
    },
    "outbounds": [
        {
            "domain_strategy": "ipv4_only",
            "network": "tcp",
            "server": "dl.gamegk.ir",
            "server_port": 443,
            "tag": "proxy",
            "tls": {
                "enabled": true
            },
            "transport": {
                "path": "/nimbaha",
                "type": "ws"
            },
            "type": "vless",
            "uuid": ""
        },
        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "block",
            "type": "block"
        },
        {
            "tag": "dns-out",
            "type": "dns"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "final": "direct",
        "rules": [
            {
                "outbound": "dns-out",
                "protocol": "dns"
            },
            {
                "outbound": "proxy",
                "ip_cidr": "10.10.0.1/24"
            }
        ]
    }
}